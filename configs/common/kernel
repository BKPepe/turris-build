# Use device tmpfs
CONFIG_KERNEL_DEVTMPFS=y
CONFIG_KERNEL_DEVTMPFS_MOUNT=y

# Provide /dev/mem
CONFIG_KERNEL_DEVMEM=y

# Enable ACL on user's request
CONFIG_USE_FS_ACL_ATTR=y

# Don't strip procfiles
CONFIG_KERNEL_PROC_STRIPPED=n

# Extend the number of supported  ath9k mPCIe cards
# by enabling PC OEM config option
CONFIG_ATH9K_SUPPORT_PCOEM=y
