From ef492d3dc5b2dbbcb8f0b6e7ef3a0b76038237da Mon Sep 17 00:00:00 2001
From: Josef Schlehofer <josef.schlehofer@nic.cz>
Date: Wed, 27 Mar 2019 02:43:17 +0100
Subject: [PATCH] asterisk{13,15}: add liburiparser as dependency

---
 net/asterisk-13.x/Makefile | 2 +-
 net/asterisk-15.x/Makefile | 2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/net/asterisk-13.x/Makefile b/net/asterisk-13.x/Makefile
index 5e4e8bf..98ff8cb 100644
--- a/net/asterisk-13.x/Makefile
+++ b/net/asterisk-13.x/Makefile
@@ -423,7 +423,7 @@ define Package/asterisk13
 $(call Package/asterisk13/Default)
   TITLE:=Complete open source PBX, v$(PKG_VERSION)
   MENU:=1
-  DEPENDS:=$(CXX_DEPENDS) +jansson +libcap +libedit +libncurses +libopenssl +libsqlite3 +libuuid +libxml2 +zlib
+  DEPENDS:=$(CXX_DEPENDS) +jansson +libcap +libedit +libncurses +libopenssl +libsqlite3 +libuuid +libxml2 +liburiparser +zlib
 endef
 
 define Package/asterisk13/description
diff --git a/net/asterisk-15.x/Makefile b/net/asterisk-15.x/Makefile
index cf8cbb4..bedef1b 100644
--- a/net/asterisk-15.x/Makefile
+++ b/net/asterisk-15.x/Makefile
@@ -428,7 +428,7 @@ define Package/asterisk15
 $(call Package/asterisk15/Default)
   TITLE:=Complete open source PBX, v$(PKG_VERSION)
   MENU:=1
-  DEPENDS:=$(CXX_DEPENDS) +jansson +libcap +libedit +libncurses +libopenssl +libsqlite3 +libuuid +libxml2 +zlib
+  DEPENDS:=$(CXX_DEPENDS) +jansson +libcap +libedit +libncurses +libopenssl +libsqlite3 +libuuid +libxml2 +liburiparser +zlib
 endef
 
 define Package/asterisk15/description
