 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/net/knot-resolver/Makefile b/net/knot-resolver/Makefile
index e4c82ba..ebc1404 100644
--- a/net/knot-resolver/Makefile
+++ b/net/knot-resolver/Makefile
@@ -32,7 +32,7 @@ define Package/knot-resolver
 	SUBMENU:=IP Addresses and Names
 	TITLE:=Knot DNS Resolver
 	URL:=https://www.knot-resolver.cz/
-	DEPENDS=+knot-libs +knot-libzscanner +libuv +luajit +dnssec-rootkey +resolver-conf +luasec +luasocket +libstdcpp +libfstrm +libprotobuf-c +lmdb
+	DEPENDS=+knot-libs +knot-libzscanner +libuv +luajit +dnssec-rootkey +resolver-conf +luasec +luasocket +libstdcpp
 	PROVIDES:=dns-resolver
 	USERID:=kresd=3536:kresd=3536
 endef
